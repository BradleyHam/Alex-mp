{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/HomeComponents/Accordion.js", "app/HomeComponents/ConnectSection.js", "app/HomeComponents/NewsArticleCard.js", "app/HomeComponents/Volunteer.js", "app/page.js", "app/HomeComponents/MissionSection.js", "app/page.js", "app/news/[article]/page.js", "app/contact/page.js", "app/news/page.js", "app/HomeComponents/NewsSection.js", "app/HomeComponents/Donate.js", "app/HomeComponents/Vote.tsx", "app/HomeComponents/GetInvolved.js", "app/HomeComponents/Policies.js", "app/about/WhyNotTories.js", "app/SiteComponents/ButtonPrimary.js", "app/SiteComponents/ButtonPrimary.js"],
  "exclude": ["node_modules"]
}
